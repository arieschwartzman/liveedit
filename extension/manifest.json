{
	"name": "Click to Remove Element",
	"version": "1.0.0",
	"manifest_version": 2,
	"description": "Jacada Live Edit",
	"icons": {
		"128": "icon_128.png",
		"48": "icon_48.png",
		"16": "icon_16.png"
	},
	"browser_action": {
		"default_icon": "icon.png"
	},
	"background": { "scripts": ["background.js"] },
	"content_scripts": [ {
		"all_frames": false,
		"js": [ "ctre_content.js" ],
		"matches": [ "http://*/*", "https://*/*" ],
		"run_at": "document_end"
	} ],
	"permissions": [
		"tabs", "contextMenus", "http://*/*"
	],
	"incognito": "spanning"
}